<?xml version="1.0" encoding="UTF-8" ?>
<environment xmlns="http://drivebuild.com"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://drivebuild.com ../drivebuild.xsd">
    <author>Ngan Nguyen</author>
    <timeOfDay>0</timeOfDay> <!-- The time of the day to set (0...1) -->

    <lanes>
        <lane> <!-- Definition of a single lane -->
            <laneSegment x="0" y="0" width="5"/> <!-- First point of the lane -->
            <laneSegment x="0" y="60" width="5"/> <!-- Second point of the lane -->
            <laneSegment x="0" y="80" width="5"/>
            <laneSegment x="0" y="120" width="5"/>
            <laneSegment x="0" y="150" width="5"/>
            <laneSegment x="0" y="200" width="5"/>
            <laneSegment x="0" y="250" width="5"/>
        </lane>
       
    </lanes>

  
</environment>
